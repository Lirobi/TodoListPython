{
  "version": 2,
  "builds": [
    {
      "src": "TodoList/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "TodoList/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "false",
    "WSGI_APPLICATION": "TodoList.wsgi.application"
  },
  "buildCommand": "pip install -r requirements.txt && gunicorn TodoList.wsgi:application --bind 0.0.0.0:$PORT",
  "outputDirectory": "."
}
